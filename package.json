{
  "name": "obsidian-4d-graph-explorer",
  "version": "0.1.0",
  "private": true,
  "description": "Obsidian plugin for immersive 4D graph visualization that supports local vault data, media, and canvas overlays.",
  "scripts": {
    "dev": "node esbuild.config.mjs --watch",
    "build": "node esbuild.config.mjs --prod",
    "test": "npm run test:unit",
    "test:unit": "rm -rf .test-dist && npx tsc -p tsconfig.test.json && node --test .test-dist/tests/**/*.test.js",
    "bench:graph": "rm -rf .test-dist && npx tsc -p tsconfig.test.json && node .test-dist/tests/bench/graphBenchmark.js"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "@types/three": "^0.160.0",
    "esbuild": "^0.21.5",
    "obsidian": "^1.5.11",
    "tslib": "^2.7.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "three": "^0.160.0"
  }
}
